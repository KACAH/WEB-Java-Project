<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Scripts/CodeMirror/lib/codemirror.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Scripts/CodeMirror/theme/eclipse.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.cookie.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
</head>

<body>
    <div id="header">
        <img src="/content/Images/java-cup-logo.png" alt="WEB Java Project Home" />
        <h1 style="padding: 10px">WEB Java Project</h1>

        <div class="lang-picker-wrapper">
            <span class="lang-picker">
                 <a class="en-US" href="javascript:void(0);">English</a>
                 <a class="lv-LV" href="javascript:void(0);">Latviešu</a>
                 <a class="ru-RU" href="javascript:void(0);">Русский</a>
            </span>
        </div>
        <script src="@Url.Content("~/Scripts/SettingsScripts/LanguageScript.js")" type="text/javascript"></script>
    </div>
    @RenderBody()
</body>
</html>
