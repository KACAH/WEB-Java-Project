@model Web_Java_Project.Models.Project

@{
    ViewBag.Title = "Compile";
    Layout = "~/Views/Shared/_ProjectLayout.cshtml";
}

<h2>@WJP_Resources.Lang.ProjectCompile - "@Html.DisplayFor(model => model.Name)"</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("CompileResult", "Project", new { ID = @Model.ProjectID }))
{
    <fieldset>
        <br/>
        <div class="editor-label">
            @WJP_Resources.Lang.CompileVersion
        </div>
        <div class="editor-field">
            <select name="version">
                <option value="J7">Java 7</option>
            </select>
        </div>
        <div class="editor-field">
            <input type="checkbox" value="Deprecation" name="deprecation" /> @WJP_Resources.Lang.CompileDeprecation
        </div>
        <div class="editor-field">
            <input type="checkbox" value="Warnings" name="warnings" /> @WJP_Resources.Lang.CompileWarnings
        </div>
        <p>
            <input type="submit" value="Compile" />
        </p>
    </fieldset>
}